# =============================================================================
# GENERAL PROJECT CONFIG
#
# This section contains general configuration settings for the Shipkit project.
# These variables control core functionalities and developer environment
# settings.
# =============================================================================

# This is the default text editor for the project. In development, this
# editor will be opened when a bug is clicked (e.g., "vscode", "zed").
REACT_EDITOR=

# The URL for the logger service, used for capturing and sending client-side
# logs in development.
NEXT_PUBLIC_LOGGER_URL=

# =============================================================================
# FEATURE FLAGS
#
# Use these flags to disable specific features, even if their environment
# variables are present. Set to "true" to disable.
# =============================================================================

# -- Core Features --
DISABLE_PWA=
DISABLE_MDX=
DISABLE_BUILDER=
DISABLE_PAYLOAD=
DISABLE_LIGHT_MODE=
DISABLE_DARK_MODE=

# -- Authentication Providers --
DISABLE_BETTER_AUTH=
DISABLE_AUTH_CLERK=
DISABLE_AUTH_STACK=
DISABLE_AUTH_CREDENTIALS=
DISABLE_AUTH_RESEND=
DISABLE_AUTH_BITBUCKET=
DISABLE_AUTH_DISCORD=
DISABLE_AUTH_GITHUB=
DISABLE_AUTH_GITLAB=
DISABLE_AUTH_GOOGLE=
DISABLE_AUTH_TWITTER=
DISABLE_AUTH_VERCEL=
DISABLE_SUPABASE_AUTH=

# -- External Services --
DISABLE_GITHUB_API=
DISABLE_GOOGLE_SERVICE_ACCOUNT=
DISABLE_LEMONSQUEEZY=
DISABLE_POLAR=
DISABLE_STRIPE=
DISABLE_S3=
DISABLE_VERCEL_BLOB=
DISABLE_REDIS=
DISABLE_VERCEL_API=

# -- AI Providers --
DISABLE_OPENAI=
DISABLE_ANTHROPIC=

# -- Analytics --
DISABLE_POSTHOG=
DISABLE_UMAMI=

# -- Consent Manager --
DISABLE_C15T=
DISABLE_CONSENT_MANAGER=


# =============================================================================
# DATABASE
#
# Configuration for the project's database connection. Shipkit supports
# PostgreSQL and can be configured to use either a local Docker container or a
# managed service like Neon.
# =============================================================================

# The connection URL for your PostgreSQL database.
# Example for local Docker: postgresql://user:password@localhost:5432/db
DATABASE_URL=

# A prefix for database-related resources, such as the database name, user,
# and password when using Docker.
DB_PREFIX=

# =============================================================================
# AUTHENTICATION
#
# This section contains configuration for authentication services, including
# Auth.js (NextAuth) and various OAuth providers.
# =============================================================================

# The base URL for your application, used by Auth.js for redirects.
AUTH_URL=

# A secret key used by Auth.js to sign and encrypt tokens.
# Generate a new secret with: `openssl rand -base64 32`
AUTH_SECRET=

# -- Better Auth --
BETTER_AUTH_SECRET=
BETTER_AUTH_BASE_URL=

# -- Clerk --
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# -- Stack Auth --
NEXT_PUBLIC_STACK_PROJECT_ID=
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=
STACK_SECRET_SERVER_KEY=


# -----------------------------------------------------------------------------
# OAuth Providers
#
# Credentials for third-party authentication providers. These are required
# if you want to enable social logins.
# -----------------------------------------------------------------------------

# Bitbucket OAuth credentials
AUTH_BITBUCKET_ID=
AUTH_BITBUCKET_SECRET=

# Discord OAuth credentials
AUTH_DISCORD_ID=
AUTH_DISCORD_SECRET=

# GitHub OAuth credentials
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

# GitLab OAuth credentials
AUTH_GITLAB_ID=
AUTH_GITLAB_SECRET=

# Google OAuth credentials
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

