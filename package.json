{
  "name": "ship-kit",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:legacy": "next build",
    "build:vercel": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "clean": "rm next-env.d.ts ; rm package-lock.json ; rm pnpm-lock.yaml ; rm bun.lockb ; rm -rf .next ; rm -rf node_modules ; rm -rf .next ; rm -rf .turbo",
    "clean:cache": "rm -rf .next/cache",
    "fix": "npm run clean && npm run payload generate:types && npm run payload generate:importmap",
    "dev": "next dev",
    "dev:legacy": "next dev",
    "dev:https": "next dev --experimental-https",
    "dev:all": "concurrently \"npm run dev:legacy\" \"npm run workers:dev\"",
    "db:generate": "drizzle-kit generate --config .config/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config .config/drizzle.config.ts",
    "db:push": "drizzle-kit push --config .config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config .config/drizzle.config.ts",
    "db:drop": "dotenv -e .env -- tsx scripts/db-drop.ts",
    "db:reset": "npm run db:drop && npm run db:generate && npm run db:migrate && npm run db:push",
    "db:sync": "dotenv -e .env -- tsx scripts/db-sync.ts",
    "db:seed": "dotenv -e .env -- tsx scripts/db-seed.ts",
    "deps:check": "ncu",
    "deps:update": "ncu -u",
    "check:metadata": "npx check-site-meta 3000",
    "check:performance": "next dev & npx react-scan@latest localhost:3000",
    "git:checkpoint": "tsx scripts/git-checkpoint.ts",
    "lint": "npm run lint:biome && npm run lint:eslint",
    "lint:biome": "biome lint . --config .config/biome.json",
    "lint:eslint": "eslint . --config .config/eslint.config.mjs",
    "lint:fix": "npm run lint:fix:biome && npm run lint:fix:eslint",
    "lint:fix:biome": "biome check --write . --config .config/biome.json",
    "lint:fix:eslint": "eslint --fix . --config .config/eslint.config.mjs",
    "start": "next start",
    "upstream:setup": "tsx scripts/setup-upstream.ts",
    "upstream:pull": "tsx scripts/git-sync-upstream.ts",
    "postinstall": "npx next telemetry disable && tsx scripts/setup-upstream.ts",
    "pull": "tsx scripts/git-sync-upstream.ts",
    "workers:build": "tsc -p .config/tsconfig.workers.json",
    "workers:dev": "concurrently \"npm run workers:build\" \"tsc -p .config/tsconfig.workers.json --watch\"",
    "test": "vitest run --config=.config/vitest.config.ts",
    "test:watch": "vitest watch --config=.config/vitest.config.ts",
    "test:coverage": "vitest run --config=.config/vitest.config.ts --coverage",
    "test:browser": "vitest --config=.config/vitest.config.browser.ts",
    "test:node": "vitest --config=.config/vitest.config.node.ts",
    "typecheck": "tsc --noEmit",
    "build:registry": "tsx src/registry/generate-registry.ts",
    "build:blocks": "tsx scripts/generate-registry.ts -i src/registry/blocks -o public/registry -t",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "build:storybook": "storybook build",
    "test:e2e": "playwright test --config=.config/playwright.config.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:both": "cross-env BUNDLE_ANALYZE=both next build",
    "trace:turbopack": "NEXT_TURBOPACK_TRACING=1 next dev",
    "trace:analyze": "next internal trace .next/trace-turbopack",
    "trace:view": "next internal turbo-trace-server .next/trace-turbopack",
    "audit:lh": "tsx scripts/lighthouse/run.ts",
    "audit:axe": "tsx scripts/a11y/axe.ts",
    "browser:open": "vibe-tools browser open \"http://localhost:3000\" --html --network --screenshot=.tmp/home.png",
    "browser:act": "URL=\"http://localhost:3000\" ACTION=\"interactive elements\" vibe-tools browser act \"$ACTION\" --url=$URL --html --network --screenshot=.tmp/act.png",
    "form-monitor": "tsx scripts/form-monitor.ts",
    "form-monitor:status": "tsx scripts/form-monitor.ts status",
    "form-monitor:test": "tsx scripts/form-monitor.ts test"
  },
